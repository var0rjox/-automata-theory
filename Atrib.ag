ATTR Prog  [  |  | d:Lista ]
ATTR Decls [  |  | d:Lista ] 
ATTR Decl  [  |  | d:Lista ]


SEM Prog 
| P1  lhs.d = @decls.d   
       

SEM Decls 
| De lhs.d = @decl.d ++ @decls.d
| Vacio lhs.d = [] 
      
SEM Decl 
| Dec lhs.d = @string:[] 
      

         




